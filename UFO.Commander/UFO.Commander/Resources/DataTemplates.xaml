<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:UFO.Commander.ViewModel"
                    xmlns:v="clr-namespace:UFO.Commander.Views"
                    xmlns:uc="clr-namespace:UFO.Commander.Views.UserControls"
                    xmlns:dialogs="clr-namespace:UFO.Commander.Views.Dialogs"
                    xmlns:local="clr-namespace:UFO.Commander.Resources">
    <DataTemplate DataType="{x:Type vm:TabControlViewModel}">
        <v:TabContentView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ArtistListViewModel}">
        <uc:ArtistList/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ArtistDialogViewModel}">
        <dialogs:ArtistDialog/>
    </DataTemplate>
</ResourceDictionary>